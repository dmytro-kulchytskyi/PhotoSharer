<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" default-lazy="true" assembly="PhotoSharer.Business" namespace="PhotoSharer.Business.Entities">
  <class name="PhotoStream" table="PhotoStreams" dynamic-update="true" >
    <cache usage="read-write"/>

    <id name="Id" column="PhotoStreamId" type="guid" generator="assigned" />

    <property name="Url" unique="true" not-null="true"/>
    <property name="Provider" unique-key="providerInfo" not-null="true"/>
    <property name="ExteralId" unique-key="providerInfo" not-null="true" />
    <property name="AccountName" not-null="false" />

    <property name="GroupId" unique-key="providerInfo" not-null="true" />
    <property name="OwnerId" not-null="true" />
     
  </class>
</hibernate-mapping>
